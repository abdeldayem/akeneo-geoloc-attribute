{
  "name": "cleverage/akeneo-geoloc-attribute",
  "license": "MIT",
  "type": "bundle",
  "description": "",
  "require": {
    "php": ">=5.4",
    "akeneo/pim-community-dev": "~1.3",
    "doctrine/migrations": "1.0.*@dev"
  },

  "require-dev": {
    "phpspec/phpspec": "~2.1"
  },

  "config": {
    "bin-dir": "bin"
  },

  "autoload": {
    "psr-4": { "CleverAge\\Bundle\\GeolocAttributeBundle\\": "src/" }
  },

  "autoload-dev": {
    "psr-4": { "CleverAge\\Bundle\\GeolocAttributeBundle\\spec\\": "spec/" }
  },

  "extra": {
    "branch-alias": {
      "dev-master": "0.2-dev"
    }
  },

  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/akeneo/pim-community-dev.git",
      "branch": "master"
    },
    {
      "type": "vcs",
      "url": "https://github.com/akeneo/platform.git",
      "branch": "master"
    }
  ]
}
